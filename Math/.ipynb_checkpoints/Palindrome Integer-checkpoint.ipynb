    "class Solution:\n",
    "    # @param A : integer\n",
    "    # @return an integer\n",
    "    def isPalindrome(self, A):\n",
    "        if A>=0:\n",
    "            rev_A = int(''.join(list(reversed(str(A)))))\n",
    "            if A == rev_A:\n",
    "                return 1\n",
    "        return 0"
   ]
